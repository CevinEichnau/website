<ul class="unstyled" >
	<% playlists.each do |p| %>
		<div class="play_list" id="play_list_<%= p.id %>" >
			<% p.links.each do |l| %>
				<br>
				<li class="draggable draggable1" id="<%= l.id %>" data-video-title="<%= l.title %>" >
					<a class="video_menu" href="#">menu</a>
					<a class="play_title" id="<%= l.video_id %>" >
						<%= l.title %>
					</a>
				</li>	
			<% end %>
		</div>
	<% end %>
</ul>		
